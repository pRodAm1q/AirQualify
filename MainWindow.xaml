<Window x:Class="AirQualify.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AirQualify"
        mc:Ignorable="d"
        Title=" AirQuality "
        Background="#F2F1F6"
        Height="600" Width="850"
        ResizeMode="NoResize" Icon=".\icons\3509012.png"
             >
 
    <Grid>
        
        <Border CornerRadius="5" BorderBrush="#000" BorderThickness="1"
        VerticalAlignment="Center" UseLayoutRounding="True" Height="50" Width="280">
            <TextBox BorderThickness="0" Background="Transparent"
                     x:Name="SearchBox"
                     Height="50"  Width="280"
                     Foreground="#a6a39a"
                      FontSize="20" TextAlignment="Center"/>
            
        </Border>
        <Button Height="50" Width="50" Margin="504,192,146,192" Content="Search" Click="SearchButton"/>
    </Grid>
</Window>
